{
  "name": "@pact-toolbox/wallet-ui",
  "version": "0.1.0",
  "description": "Cross-framework wallet UI components for pact-toolbox",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js",
      "require": "./dist/vue.cjs"
    },
    "./angular": {
      "types": "./dist/angular.d.ts",
      "import": "./dist/angular.js",
      "require": "./dist/angular.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@pact-toolbox/crypto": "workspace:*",
    "@pact-toolbox/signers": "workspace:*",
    "@pact-toolbox/types": "workspace:*",
    "@pact-toolbox/ui-shared": "workspace:*",
    "@pact-toolbox/wallet-adapters": "workspace:*",
    "@pact-toolbox/wallet-core": "workspace:*",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@angular/core": "^20.1.6",
    "@pact-toolbox/tsdown": "workspace:*",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "happy-dom": "^18.0.1",
    "oxlint": "^1.11.0",
    "react": "^19.1.1",
    "rxjs": "^7.8.2",
    "tsdown": "^0.13.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.18"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "keywords": [
    "kadena",
    "pact",
    "wallet",
    "web3",
    "web-components",
    "lit-element"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/salamaashoush/pact-toolbox.git",
    "directory": "packages/wallet-ui"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
