{
  "name": "@pact-toolbox/proxy",
  "version": "0.0.6",
  "repository": "kadena-community/pact-toolbox",
  "author": "Salama Ashoush <salamaashoush@gmail.com>",
  "license": "MIT",
  "type": "module",
  "private": false,
  "main": "./src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup && cp -r ./configs ./dist/configs",
    "dev": "tsup --watch",
    "lint": "oxlint --import-plugin --deny-warnings",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "prepack": "pnpm run build",
    "release": "pnpm test && pnpm publish",
    "test:watch": "vitest",
    "test": "vitest run",
    "verify": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@pact-toolbox/config": "workspace:*",
    "@pact-toolbox/utils": "workspace:*",
    "h3": "^1.11.1",
    "listhen": "^1.7.2"
  },
  "devDependencies": {
    "@pact-toolbox/prettier-config": "workspace:*",
    "@pact-toolbox/tsconfig": "workspace:*",
    "@vitest/coverage-v8": "^1.6.0",
    "lint-staged": "^15.2.5",
    "oxlint": "^0.4.3",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "prettier": "@pact-toolbox/prettier-config"
}
