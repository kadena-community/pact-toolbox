{
  "name": "@pact-toolbox/network-config",
  "version": "0.1.0",
  "description": "Network configuration management for pact-toolbox",
  "type": "module",
  "repository": "https://github.com/salamaashoush/pact-toolbox",
  "homepage": "https://github.com/salamaashoush/pact-toolbox",
  "bugs": "https://github.com/salamaashoush/pact-toolbox/issues",
  "author": "Salama Ashoush",
  "license": "MIT",
  "keywords": [
    "pact",
    "kadena",
    "network",
    "configuration"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdown --config ./node_modules/@pact-toolbox/tsdown/tsdown.config.lib.ts",
    "dev": "tsdown --watch --config ./node_modules/@pact-toolbox/tsdown/tsdown.config.lib.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@pact-toolbox/types": "workspace:*"
  },
  "devDependencies": {
    "@pact-toolbox/tsconfig": "workspace:*",
    "@pact-toolbox/tsdown": "workspace:*",
    "@pact-toolbox/vitest-config": "workspace:*",
    "@types/node": "^24.2.0",
    "tsdown": "^0.13.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
