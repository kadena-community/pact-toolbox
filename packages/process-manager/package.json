{
  "name": "@pact-toolbox/process-manager",
  "version": "0.1.0",
  "description": "Modern process management and orchestration for Pact Toolbox",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
