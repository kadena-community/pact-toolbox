{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-coverage", "typedoc-plugin-merge-modules"],
  "readme": "none",
  "githubPages": false,
  "hideGenerator": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "skipErrorChecking": true,
  "treatWarningsAsErrors": false,
  "cleanOutputDir": false,
  "useCodeBlocks": true,
  "expandObjects": false,
  "hidePageTitle": true,
  "disableSources": true,
  "fileExtension": ".md",
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/test/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/examples/**",
    "**/templates/**"
  ]
}
